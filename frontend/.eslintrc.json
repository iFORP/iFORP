{
  "parser": "babel-eslint",
  "extends": [ "baumeister", "prettier" ],
  "plugins": [ "react", "prettier" ],
  "rules": {
    "no-console": "off",
    "react/jsx-uses-react": "error",
    "react/jsx-uses-vars": "error",
    "unicorn/filename-case": "off",
    "unicorn/no-process-exit": "off"
  },
  "overrides": [
    {
      "files": ["build/*.js"]
    }
  ]
}
