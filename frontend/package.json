{
  "name": "iforp-frontend",
  "title": "iFORP Frontend",
  "description": "iFORP Frontend",
  "version": "0.0.0",
  "private": true,
  "homepage": "http://prototyping4innovation.de",
  "license": "MIT",
  "main": "src",
  "author": {
    "name": "Michael Kühnel",
    "email": "m.kuehnel@micromata.de",
    "url": "http://micromata.de"
  },
  "contributors": [
    {
      "name": "Nicolai Süper",
      "email": "n.sueper@micromata.de",
      "url": "http://micromata.de"
    }
  ],
  "repository": "micromata/iforp",
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "start": "npm-run-all clean:dev handlebars --parallel handlebars:watch webpack:server eslint:watch stylelint:watch --silent",
    "build": "npm-run-all security test handlebars clean webpack --silent",
    "build:dev": "npm-run-all security test handlebars clean:dev webpack:dev --silent",
    "build:serve": "serve dist --open",
    "build:analyze": "npm run webpack:stats --silent && webpack-bundle-analyzer ./.webpack-stats.json ./dist -s parsed",
    "pretest": "npm run eslint --silent",
    "test": "cross-env NODE_ENV=test jest",
    "test:watch": "npm run test -- --watch",
    "eslint": "eslint --ext .jsx,.js --cache --ignore-path .gitignore --ignore-pattern '/src/library/*' build src ",
    "eslint:fix": "npm run eslint --silent -- --fix",
    "eslint:watch": "onchange \"src/**/*.js\" \"src/**/*.jsx\" \"build/**/*.js\" -- onerror \"npm run eslint --silent\" -t \"Linting error(s)\"",
    "stylelint": "stylelint \"src/assets/scss/**/*.scss\"",
    "stylelint:fix": "npm run stylelint --silent -- --fix",
    "stylelint:watch": "onchange \"src/assets/scss/**/*.scss\" -- onerror \"npm run stylelint --silent\" -t \"Linting error(s)\"",
    "release": "npm run build --silent && standard-version --tag-prefix",
    "release:patch": "npm run release --silent -- --release-as patch",
    "release:minor": "npm run release --silent -- --release-as minor",
    "release:major": "npm run release --silent -- --release-as major",
    "webpack": "cross-env NODE_ENV=production webpack --config build/webpack.config.babel.js --hide-modules --progress",
    "webpack:dev": "cross-env NODE_ENV=development webpack --config build/webpack.config.babel.js --hide-modules --progress",
    "webpack:server": "cross-env NODE_ENV=development webpack-dev-server --config build/webpack.config.babel.js --open --host 0.0.0.0",
    "webpack:stats": "cross-env NODE_ENV=production webpack --config build/webpack.config.babel.js --profile --json > ./.webpack-stats.json",
    "handlebars": "babel-node build/handlebars.js",
    "handlebars:watch": "onchange \"src/**/*.hbs\" \"src/handlebars/helpers/*.js\" -- onerror \"npm run handlebars --silent\" -t \"Handlebars build failed\"",
    "security": "nsp check",
    "clean": "del .webpack-assets.json dist",
    "clean:dev": "del .webpack-assets.json server",
    "postmerge": "npm install"
  },
  "dependencies": {
    "axios": "0.18.0",
    "bootstrap": "4.1.1",
    "core-js": "2.5.7",
    "promise-polyfill": "8.0.0",
    "prop-types": "15.6.1",
    "react": "16.4.0",
    "react-archer": "0.2.0",
    "react-dom": "16.4.0",
    "react-router-dom": "4.3.1",
    "reactstrap": "6.1.0",
    "unfetch": "3.0.0"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-eslint": "8.2.3",
    "babel-loader": "7.1.4",
    "babel-plugin-syntax-dynamic-import": "6.18.0",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-imports": "1.5.0",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-plugin-transform-react-jsx": "6.24.1",
    "babel-plugin-transform-regenerator": "6.26.0",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-env": "1.7.0",
    "chalk": "2.4.1",
    "cli-error-notifier": "2.0.1",
    "common-tags": "1.8.0",
    "copy-webpack-plugin": "4.5.1",
    "cross-env": "5.1.6",
    "css-loader": "0.28.11",
    "del-cli": "1.1.0",
    "eslint": "4.19.1",
    "eslint-config-baumeister": "1.0.4",
    "eslint-config-prettier": "2.9.0",
    "eslint-config-xo": "0.22.2",
    "eslint-plugin-filenames": "1.3.0",
    "eslint-plugin-import": "2.12.0",
    "eslint-plugin-prettier": "2.6.0",
    "eslint-plugin-react": "7.9.1",
    "eslint-plugin-security": "1.4.0",
    "eslint-plugin-unicorn": "4.0.3",
    "file-loader": "1.1.11",
    "globby": "8.0.1",
    "husky": "0.14.3",
    "imagemin-webpack-plugin": "2.1.5",
    "jest-cli": "23.1.0",
    "jstransformer-handlebars": "1.1.0",
    "log-symbols": "2.2.0",
    "metalsmith": "2.3.0",
    "metalsmith-discover-partials": "0.1.0",
    "metalsmith-filter": "1.0.2",
    "metalsmith-in-place": "4.1.1",
    "metalsmith-layouts": "2.1.0",
    "metalsmith-register-helpers": "0.4.0",
    "mini-css-extract-plugin": "0.4.0",
    "minimist": "1.2.0",
    "node-sass": "4.9.0",
    "npm-run-all": "4.1.3",
    "nsp": "3.2.1",
    "onchange": "4.0.0",
    "perfy": "1.1.2",
    "postcss-loader": "2.1.5",
    "prettier": "1.13.5",
    "purify-css": "1.2.6",
    "purifycss-webpack": "0.7.0",
    "regenerator": "0.13.1",
    "sass-loader": "7.0.3",
    "serve": "8.2.0",
    "standard-version": "4.4.0",
    "style-loader": "0.21.0",
    "stylelint": "9.2.1",
    "stylelint-config-standard": "18.2.0",
    "webpack": "4.12.0",
    "webpack-assets-manifest": "3.0.1",
    "webpack-bundle-analyzer": "2.13.1",
    "webpack-cli": "3.0.4",
    "webpack-dev-server": "3.1.4"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "<rootDir>/build/",
      "<rootDir>/node_modules/"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/app/**/*.{js,jsx}",
      "!**/node_modules/**"
    ],
    "coverageReporters": [
      "lcov",
      "text",
      "text-summary"
    ]
  }
}
