{
  "useHandlebars": false,
  "purifyCSS": {
    "usePurifyCSS": false,
    "whitelist": [
      "*navbar*",
      "*modal*",
      "*dropdown*",
      "*carousel*",
      "*tooltip*",
      "open",
      "fade",
      "collapse",
      "collapsing",
      "in"
    ]
  },
  "generateBanners": false,
  "cacheBusting": true,
  "vendor": {
    "bundleCSS": [],
    "includeStaticFiles": []
  },
  "webpack": {
    "DefinePlugin": {
      "development": {
        "PRODUCTION": false,
        "API_PREFIX": "'http://localhost:8087/api/'"
      },
      "production": {
        "PRODUCTION": true,
        "API_PREFIX": "'http://localhost:8087/api/'"
      }
    },
    "ProvidePlugin": {}
  }
}
