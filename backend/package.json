{
  "name": "iforp-backend",
  "description": "",
  "version": "0.0.0",
  "homepage": "",
  "main": "src",
  "keywords": [
    "feathers"
  ],
  "author": {
    "name": "Michael KÃ¼hnel",
    "email": "m.kuehnel@micromata.de"
  },
  "contributors": [],
  "bugs": {},
  "directories": {
    "lib": "src",
    "test": "test/"
  },
  "engines": {
    "node": "^8.0.0",
    "npm": ">= 3.0.0"
  },
  "scripts": {
    "test": "npm run eslint && jest",
    "eslint": "eslint src/**/** --config .eslintrc.json --ignore-path .gitignore",
    "eslint:fix": "npm run eslint --silent -- --fix",
    "start": "ts-node src/index.ts",
    "start:dev": "nodemon src/index.ts",
    "build": "tsc -p .",
    "database:dummyData": "npm run database:delete && echo 'Database deleted' && ts-node src/orm/dummy-data",
    "database:delete": "del '*.sqlite'",
    "pretty": "prettier --write \"./src/**/*.{js?(x),ts?(x)}\""
  },
  "dependencies": {
    "@sindresorhus/slugify": "0.3.0",
    "body-parser": "1.18.3",
    "cheerio": "1.0.0-rc.2",
    "cors": "2.8.4",
    "express": "4.16.3",
    "fs-extra": "6.0.1",
    "multer": "^1.3.0",
    "pify": "^3.0.0",
    "reflect-metadata": "0.1.12",
    "sqlite3": "4.0.0",
    "superb": "2.0.0",
    "typeorm": "0.2.5",
    "yauzl": "^2.9.1"
  },
  "devDependencies": {
    "@types/cheerio": "0.22.7",
    "@types/express": "4.11.1",
    "@types/jest": "22.2.3",
    "@types/node": "8.0.29",
    "del-cli": "1.1.0",
    "eslint": "^4.19.1",
    "eslint-config-baumeister": "^1.0.2",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-xo": "^0.22.2",
    "eslint-plugin-filenames": "^1.2.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jest": "21.15.1",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-typescript": "^0.12.0",
    "eslint-plugin-unicorn": "^4.0.3",
    "husky": "1.0.0-rc.6",
    "jest": "22.4.3",
    "lint-staged": "7.1.0",
    "nodemon": "1.17.4",
    "prettier": "1.12.1",
    "ts-jest": "22.4.6",
    "ts-node": "3.3.0",
    "tsc": "1.20150623.0",
    "typescript": "^2.8.3",
    "typescript-eslint-parser": "^15.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "npm run pretty",
      "npm run eslint:fix",
      "git add"
    ]
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.(j|t)s?(x)",
      "!src/index.ts"
    ],
    "testMatch": [
      "**/__tests__/**/*.(j|t)s?(x)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
